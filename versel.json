{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/upload/image",
        "methods": ["POST"],
        "dest": "/api/upload/image"
      },
      {
        "src": "/upload/video",
        "methods": ["POST"],
        "dest": "/api/upload/video"
      },
      {
        "src": "/gateway/:hash",
        "methods": ["GET"],
        "dest": "/api/gateway/:hash"
      }
    ]
  }